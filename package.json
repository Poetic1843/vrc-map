{
    "name": "vrc-map",
    "description": "A map of the field for the VRC game Tipping Point. This map is ideal for doing things like planning skills routes and \r for making images for the notebook. You can try it out at https://poetic1843.github.io/vrc-map/",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "publish": "npm run build && node scripts/publish.js",
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "start": "sirv public --no-clear",
        "check": "svelte-check --tsconfig ./tsconfig.json"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^22.0.0",
        "@rollup/plugin-node-resolve": "^13.2.1",
        "@rollup/plugin-typescript": "^8.0.0",
        "@tsconfig/svelte": "^3.0.0",
        "@rollup/plugin-replace": "^4.0.0",
        "rollup": "^2.3.4",
        "rollup-plugin-css-only": "^3.1.0",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-svelte": "^7.0.0",
        "rollup-plugin-terser": "^7.0.0",
        "svelte": "^3.0.0",
        "svelte-check": "^2.0.0",
        "svelte-preprocess": "^4.0.0",
        "tslib": "^2.4.0",
        "typescript": "^4.0.0",
        "gh-pages": "^3.2.3",
        "sirv-cli": "^2.0.0"
    }
}
